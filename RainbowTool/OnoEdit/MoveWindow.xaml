<Window x:Class="OnoEdit.MoveWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ono="clr-namespace:OnoEdit"
        Title="Moves" Height="500" Width="800">
        <DockPanel>
            <ono:OnoGrid x:Name="Grid" DataContext="{Binding Path=BCMFile.Moves, Source={StaticResource OpenedFiles}}" FrozenColumns="1"/>
        </DockPanel>
</Window>
